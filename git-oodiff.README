  mkdir <PACKAGE-YYYY-MM-DD>
  cd YYYY-MM-DD
  wget <URL>
  cd ..
  tar -jcf <PACKAGE-YYYY-MM-DD>.tar.bz2 <PACKAGE-YYYY-MM-DD>

